{
  "name": "vibe-coding",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings=0",
    "format": "prettier --write .",
    "check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "next": "16.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.8",
    "@vitest/coverage-v8": "^4.0.15",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4",
    "typescript": "^5",
    "@netlify/plugin-nextjs": "^5",
    "vitest": "^4.0.15",
    "jsdom": "^25.0.1",
    "husky": "^9",
    "lint-staged": "^15"
  },
  "engines": {
    "node": "24.x"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ],
    "*.{js,jsx,ts,tsx,css,md,json,mdx,yml,yaml}": [
      "prettier --write"
    ]
  }
}
